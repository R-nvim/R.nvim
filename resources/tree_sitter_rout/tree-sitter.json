{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "rout",
      "camelcase": "Rout",
      "title": "Rout",
      "scope": "source.rout",
      "file-types": [
        "rout"
      ],
      "injection-regex": "^rout$",
      "class-name": "TreeSitterRout"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "R output",
    "authors": [
      {
        "name": "Jakson Aquino",
        "email": "jalvesaq@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/R-nvim/tree-sitter-rout"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}